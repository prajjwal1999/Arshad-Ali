#include<bits/stdc++.h>
using namespace std;
#define ll long long int

int main()
{
  #ifndef ONLINE_JUDGE
  freopen("input.txt","r",stdin);
  freopen("output.txt","w",stdout);
  #endif
  ll n;
  cin>>n;
  char s[n];
  cin>>s;
  map<char,int> mp;
  for(int i=0;i<n;i++)
  {
    mp[s[i]++];
  }
  for(auto i:mp)
  {
    cout<<i.second<<" ";
  }

  return 0;

}
